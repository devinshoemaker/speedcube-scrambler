{
  "name": "cube-scrambler",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/cube-scrambler/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/cube-scrambler",
        "main": "libs/cube-scrambler/src/index.ts",
        "tsConfig": "libs/cube-scrambler/tsconfig.lib.json",
        "assets": ["libs/cube-scrambler/*.md"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs cube-scrambler {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/cube-scrambler/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/libs/cube-scrambler"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/libs/cube-scrambler"
      }
    }
  },
  "tags": []
}
